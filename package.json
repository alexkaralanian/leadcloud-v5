{
  "name": "server",
  "version": "0.5.0",
  "main": "index.js",
  "engines": {
    "node": "8.10.0",
    "npm": "5.6.0"
  },
  "scripts": {
    "start": "sh start.sh",
    "server": "node index.sjs",
    "dev:server": "nodemon index.js",
    "dev:client": "npm start --prefix client",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "npm run build --prefix client",
    "test:client": "npm run test --prefix client",
    "test:server": "jest tests --detectOpenHandles",
    "test": "CI=true npm run test:server && npm run test:client",
    "migrate": "sequelize db:migrate",
    "rollback": "sequelize db:migrate:undo",
    "analyze": "npm run analyze --prefix client"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.244.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "connect-redis": "^3.3.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "googleapis": "^27.0.0",
    "helmet": "^3.12.0",
    "jest": "^23.1.0",
    "lodash.isempty": "^4.4.0",
    "mailparser": "^2.2.0",
    "mime": "^2.3.1",
    "mocha": "^5.0.4",
    "moment": "^2.22.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.2",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "puppeteer": "^1.5.0",
    "redis": "^2.8.0",
    "sequelize": "^4.37.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.3",
    "prettier": "1.11.1"
  }
}
