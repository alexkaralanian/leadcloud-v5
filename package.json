{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "8.10.0",
    "npm": "5.6.0"
  },
  "scripts": {
    "start": "node index.js",
    "pm2-start": "pm2 start index.js -i 0",
    "postinstall": "npm i --prefix client && npm run build",
    "dev":
      "concurrently \"nodemon index.js\" \"npm run start --prefix client\"",
    "build": "npm run build --prefix client",
    "test": "npm run test --prefix client",
    "migrate": "sequelize db:migrate",
    "rollback": "sequelize db:migrate:undo"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "connect-redis": "^3.3.3",
    "cookie-session": "^2.0.0-beta.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "googleapis": "^27.0.0",
    "helmet": "^3.12.0",
    "mailparser": "^2.2.0",
    "mocha": "^5.0.4",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.2",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "redis": "^2.8.0",
    "sequelize": "^4.37.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "chai-http": "^3.0.0",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.3",
    "prettier": "1.11.1",
    "supertest": "^3.0.0",
    "supertest-session": "^3.1.2"
  },
  "jest": {
    "collectCoverageFrom": ["*.js"],
    "globals": {
      "statement": 50,
      "branches": 50,
      "functions": 50,
      "lines": 100
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
