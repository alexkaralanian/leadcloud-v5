{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["jest", "prettier", "react"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true,
    "jest/globals": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "babel/semi": 1,
    "prettier/prettier": "error",
    "import/no-named-as-default": 0,
    "no-underscore-dangle": 0
  },
}

