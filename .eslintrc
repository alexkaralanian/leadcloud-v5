{
  "extends": ["airbnb", "prettier", "prettier/react"],

  "plugins": ["jest", "prettier", "react"],

  "parser": "babel-eslint",

  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },

  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true,
    "jest/globals": true
  },

  "rules": {
    "prettier/prettier": ["error"]
  }
}

